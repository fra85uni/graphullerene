&GLOBAL
  PROJECT pentahyb11
  RUN_TYPE GEO_OPT
  PRINT_LEVEL LOW
 EXTENDED_FFT_LENGTHS
&END GLOBAL
&FORCE_EVAL
  METHOD Quickstep
!  STRESS_TENSOR  ANALYTICAL
!  &PRINT
!    &STRESS_TENSOR
!    &END STRESS_TENSOR
!  &END PRINT
  &DFT
            BASIS_SET_FILE_NAME  /g100_work/IscrC_Reo-Solv/Basis-pseudo/BASIS_ADMM
            BASIS_SET_FILE_NAME /g100_work/IscrC_Reo-Solv/Basis-pseudo/BASIS_ADMM_MOLOPT
            BASIS_SET_FILE_NAME /g100_work/IscrC_Reo-Solv/Basis-pseudo/BASIS_MOLOPT
            BASIS_SET_FILE_NAME /g100_work/IscrC_Reo-Solv/Basis-pseudo/GTH_POTENTIALS
            BASIS_SET_FILE_NAME /g100_work/IscrC_Reo-Solv/Basis-pseudo//BASIS_RI_cc-TZ
! WFN Restart from GGA
           WFN_RESTART_FILE_NAME /g100_work/IscrC_Def-OCS/hole/el-RESTART.wfn
!Activate for spin-polarized
            UKS
            CHARGE +1
    &MGRID
      CUTOFF 600
    &END MGRID
    &QS
      METHOD GPW
    &END QS
    &SCF
      SCF_GUESS  RESTART
      MAX_SCF    100
      &OT
        MINIMIZER       CG
        PRECONDITIONER  FULL_SINGLE_INVERSE
      &END
      &OUTER_SCF
!        EPS_SCF  1.0E-7
!        MAX_SCF  50
      &END
!      &PRINT
!        &RESTART OFF
!        &END
!      &END
    &END SCF
    &AUXILIARY_DENSITY_MATRIX_METHOD
        METHOD BASIS_PROJECTION
        ADMM_PURIFICATION_METHOD NONE
     &END AUXILIARY_DENSITY_MATRIX_METHOD
    &PRINT
!       &MO_CUBES
!        NHOMO 1
!        NLUMO 1
!       WRITE_CUBE F
!      &END MO_CUBES
!       &TOT_DENSITY_CUBE
!       &END
    &END PRINT
#These are the coefficients used for PBE0
&XC
 &XC_FUNCTIONAL
  &PBE
   SCALE_X 0.70
   SCALE_C 1.0
 &END PBE
 &PBE_HOLE_T_C_LR
 CUTOFF_RADIUS 6.00000
 SCALE_X 0.30
 &END PBE_HOLE_T_C_LR
&END XC_FUNCTIONAL
&HF
&SCREENING
EPS_SCHWARZ 1.0E-6
SCREEN_ON_INITIAL_P FALSE
&END SCREENING
&INTERACTION_POTENTIAL
POTENTIAL_TYPE TRUNCATED
CUTOFF_RADIUS 6.00000
T_C_G_DATA /g100_work/IscrC_Reo-Solv/Basis-pseudo/t_c_g.dat
&END INTERACTION_POTENTIAL
&MEMORY
MAX_MEMORY 3600
EPS_STORAGE_SCALING 0.1
&END MEMORY
FRACTION 0.30
&END HF
      ! non-local vdW part
      &vdW_POTENTIAL
         DISPERSION_FUNCTIONAL NON_LOCAL
         &NON_LOCAL
           TYPE RVV10
           PARAMETERS  7.8  0.0093
           KERNEL_FILE_NAME   /g100_work/IscrC_Reo-Solv/Basis-pseudo/rVV10_kernel_table.dat
           CUTOFF      400
         &END NON_LOCAL
      &END vdW_POTENTIAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
A  28.5200    0.00000   0.00000
B  0.00000   28.5200    0.00000
C  0.00000   0.00000   28.5200
    &END CELL
    &TOPOLOGY
      COORD_FILE_NAME   ./C60.xyz
      COORD_FILE_FORMAT xyz
    &END TOPOLOGY
    &KIND C
      BASIS_SET DZVP-MOLOPT-GTH-q4
      BASIS_SET AUX_FIT cFIT3 !SZV-MOLOPT-SR-GTH
      POTENTIAL GTH-PBE-q4
    &END KIND
    &KIND H
      BASIS_SET DZVP-MOLOPT-SR-GTH-q1
      BASIS_SET AUX_FIT cFIT3 !SZV-MOLOPT-SR-GTH
    POTENTIAL GTH-PBE-q1
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&MOTION
&CONSTRAINT
 &FIXED_ATOMS
  LIST 1..1860
 &END FIXED_ATOMS
&END CONSTRAINT
&GEO_OPT
  OPTIMIZER BFGS
    &BFGS
     TRUST_RADIUS 0.1
    &END
&END GEO_OPT
        &END MOTION
!&EXT_RESTART
! RESTART_FILE_NAME   /g100_work/IscrC_Def-OCS/hole/pentahyb11-1.restart
!&END EXT_RESTART
